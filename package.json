{
  "name": "@tiledb-inc/pybabylonjs",
  "version": "0.1.8",
  "description": "BabylonJS widget",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/TileDB-Inc/PyBabylonJS",
  "bugs": {
    "url": "https://github.com/TileDB-Inc/PyBabylonJS/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "TileDB",
    "email": "support@tiledb.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TileDB-Inc/PyBabylonJS"
  },
  "scripts": {
    "build": "yarn run build:lib && yarn run build:nbextension && yarn run build:labextension",
    "build:prod": "yarn run build:lib && yarn run build:nbextension && yarn run build:labextension",
    "build:labextension": "yarn run clean:labextension && mkdirp pybabylonjs/labextension && cd pybabylonjs/labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:nbextension": "NODE_OPTIONS=--max-old-space-size=4096 webpack --mode=production",
    "clean": "yarn run clean:lib && yarn run clean:nbextension && yarn run clean:labextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf pybabylonjs/labextension",
    "clean:nbextension": "rimraf pybabylonjs/nbextension/static/index.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "prepack": "yarn run build:lib",
    "test": "echo test",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "NODE_OPTIONS=--max-old-space-size=4096 webpack --watch --mode=development"
  },
  "dependencies": {
    "@babylonjs/core": "^4.2.0",
    "@jupyter-widgets/base": "^1.1.10 || ^2 || ^3",
    "@tiledb-inc/tiledb-cloud": "^0.0.8"
  },
  "devDependencies": {
    "@babylonjs/core": "^4.2.0",
    "@types/node": "^10.11.6",
    "@types/webpack-env": "^1.16.2",
    "crypto": "1.0.1",
    "css-loader": "^6.3.0",
    "expect.js": "^0.3.1",
    "fs-extra": "^7.0.0",
    "git-tag-version": "^1.3.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
